{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Product Manager System",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "darkaonline/l5-swagger": "^8.3",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "barryvdh/laravel-ide-helper": "@stable",
        "friendsofphp/php-cs-fixer": "@stable",
        "nunomaduro/larastan": "@stable",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "User\\": "../User/",
            "UseCases\\": "../UseCases/",
            "Auth\\": "../Auth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcbf": "phpcbf --standard=phpcs.xml app tests",
        "phpcs": "phpcs --standard=phpcs.xml app tests",
        "phpmd": "phpmd app,tests text codesize,controversial",
        "ci": [
            "@phpmd",
            "@phpcs",
            "@phpstan"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fixer": "php-cs-fixer fix --config=../.php-cs-fixer.dist.php --allow-risky=yes --diff",
        "open-api": [
            "@php artisan l5-swagger:generate"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
